{
  "unsplash": {
    "input": {
      "target": "./opapi/unsplashApiSpec.json"
    },
    "output": {
      "target": "./generated/unsplashApi.ts",
      "schemas": "./generated/model",
      "client": "react-query",
      "override": {
        "mutator": {
          "path": "./customInstance.ts",
          "name": "customInstance"
        },
        "query": {
          "useQuery": true,
          "useInfinite": true,
          "useInfiniteQueryParam": "page"
        }
      }
    }
  }
} 